{
    "name": "atduino",
    "version": "1.1.1",
    "description": "Interfaces to or masquerades as a modem using V.25 AT commands",
    "keywords": "at, at command, modem",
    "license": "MIT",
    "frameworks": "arduino",
    "platforms": ["espressif32"],
    "repository": {
        "type": "git",
        "url": "https://github.com/gbrucepayne/atduino.git"
    },
    "dependencies": {
        "gbrucepayne/ardebug": "https://github.com/gbrucepayne/ardebug.git"
    },
    "examples": [
        {
            "name": "Client",
            "base": "examples/client",
            "files": [
                "main.cpp",
                "include/atclient.h",
                "src/atclient.cpp"
            ]
        },
        {
            "name": "Server",
            "base": "examples/server",
            "files": [
                "main.cpp",
                "include/atserver.h",
                "src/atserver.cpp"
            ]
        }
    ]
}